{
	"repositories": [
		{ "type": "composer", "url": "https://composer.typo3.org/" }
	],
	"name": "typo3/cms-base-distribution",
	"description" : "TYPO3 CMS Base Distribution",
	"license": "GPL-2.0+",
	"require": {
		"typo3/cms": "^7.6"
	},
	"extra": {
		"typo3/cms": {
			"cms-package-dir": "{$vendor-dir}/typo3/cms",
			"web-dir": "web"
		}
	},
	"autoload": {
		"classmap": [
			"web/typo3conf/ext/caretaker_instance/classes/",
			"web/typo3conf/ext/caretaker_instance/services/"
		]
	}
}
