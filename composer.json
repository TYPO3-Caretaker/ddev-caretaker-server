{
	"repositories": [
		{ "type": "composer", "url": "https://composer.typo3.org/" }
	],
	"name": "caretaker/ddev-caretaker-instance",
	"description" : "TYPO3 CMS Base Distribution",
	"license": "GPL-2.0-or-later",
	"require": {
		"typo3/cms-base-distribution": "^9.5",
		"helhum/typo3-console": "5.7.2",
		"typo3/cms-lowlevel": "^9",
		"typo3/cms-reports": "^9"
	},
	"scripts":{
		"typo3-cms-scripts": [
			"typo3cms install:fixfolderstructure",
			"typo3cms install:generatepackagestates"
		],
		"post-autoload-dump": [
			"@typo3-cms-scripts"
		]
	},
	"extra": {
		"typo3/cms": {
			"web-dir": "public"
		}
	},
	"autoload": {
		"classmap": [
			"public/typo3conf/ext/caretaker_instance/classes/",
			"public/typo3conf/ext/caretaker_instance/services/",
			"public/typo3conf/ext/caretaker/classes/",
			"public/typo3conf/ext/caretaker/Classes/",
			"public/typo3conf/ext/caretaker/scheduler/",
			"public/typo3conf/ext/caretaker/interfaces/"
		]
	}
}
